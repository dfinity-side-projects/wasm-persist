    "leb128": "0.0.4",
    "wasm-json-toolkit": "^0.3.0"
  },
  "standard": {
    "globals": [
      "WebAssembly"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "documentation": "^6.3.1",
    "minimist": "^1.2.0",
    "nyc": "^11.7.3",
    "standard": "^11.0.1",
    "tape": "^4.9.0",
    "wabt": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dfinity/wasm-persist.git"
  },
  "bugs": {
    "url": "https://github.com/dfinity/wasm-persist/issues"
  },
  "homepage": "https://github.com/dfinity/wasm-persist"
